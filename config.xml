<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="io.cristianbara.github.RRDOuting" version="1.0.0">
  <name>RRD Outing</name>
  <description>Riddle me this. Riddle me that. Find the key to get out.</description>
  <author href="http://cristianbara.github.io" email="cristian.dan.bara@gmail.com">CB web dev.</author>
  <content src="index.html"/>
  <preference name="permissions" value="none"/>
  <preference name="orientation" value="landscape"/>
  <preference name="target-device" value="tablet"/>
  <preference name="fullscreen" value="false"/>
  <preference name="webviewbounce" value="true"/>
  <preference name="prerendered-icon" value="true"/>
  <preference name="stay-in-webview" value="true"/>
  <preference name="ios-statusbarstyle" value="black-opaque"/>
  <preference name="detect-data-types" value="true"/>
  <preference name="exit-on-suspend" value="false"/>
  <preference name="show-splash-screen-spinner" value="true"/>
  <preference name="auto-hide-splash-screen" value="true"/>
  <preference name="disable-cursor" value="false"/>
  <preference name="android-minSdkVersion" value="7"/>
  <preference name="android-installLocation" value="preferExternal"/>
  
  
 
  
  <icon src="icon.png"/>
  <icon src="res/icon/icon.png" gap:platform="android" gap:qualifier="ldpi"/>
  <icon src="res/icon/icon.png" gap:platform="android" gap:qualifier="mdpi"/>
  <icon src="res/icon/icon.png" gap:platform="android" gap:qualifier="hdpi"/>
  <icon src="res/icon/icon.png" gap:platform="android" gap:qualifier="xhdpi"/>
  <icon src="res/icon/icon.png" gap:platform="blackberry"/>
  <icon src="res/icon/icon.png" gap:platform="blackberry" gap:state="hover"/>
  <icon src="res/icon/icon.png" gap:platform="ios" width="57" height="57"/>
  <icon src="res/icon/icon.png" gap:platform="ios" width="72" height="72"/>
  <icon src="res/icon/icon.png" gap:platform="ios" width="114" height="114"/>
  <icon src="res/icon/icon.png" gap:platform="ios" width="144" height="144"/>
  <icon src="res/icon/icon.png" gap:platform="webos"/>
  <icon src="res/icon/icon.png" gap:platform="winphone"/>
  <icon src="res/icon/icon.png" gap:platform="winphone" gap:role="background"/>
  
  
  <plugin name="cordova-plugin-whitelist" version="1"/>
  
  
  <platform name="android">
    <allow-intent href="market:*"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
</widget>